<?xml version="1.0" encoding="utf-8"?>
<!-- rounded_border_gray.xml -->
<shape xmlns:android="http://schemas.android.com/apk/res/android">
    <solid android:color="@android:color/transparent" /> <!-- 设置填充颜色为透明，否则会覆盖 CardView 内容 -->
    <stroke
        android:width="1dp"
        android:color="#99D3D3D3" /> <!-- 设置边框宽度和颜色 -->
    <corners android:radius="12dp" /> <!-- 设置圆角半径，这里设置为 8dp -->
</shape>
